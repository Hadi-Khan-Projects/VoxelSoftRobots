<mujoco>

    <compiler autolimits="true"/>
    <include file="scene.xml"/>
    <option solver="Newton" tolerance="1e-6" timestep=".001" integrator="implicitfast"/>
    <size memory="1000M"/>

    poisson

    <worldbody>
        <!-- flexcomps of type grid -->
        <flexcomp name="flexcomp_grid_1" type="grid" count="4 4 4" spacing=".2 .2 .2" pos="-3 1 1.5"
                  radius=".005" rgba=".7 .7 0 1" dim="3" mass="5">
            <contact condim="3" solref="0.01 1" solimp=".95 .99 .0001" selfcollide="none"/>
            <edge damping="1"/>
            <elasticity young="1e4" poisson="0.4"/>
        </flexcomp>

        <flexcomp name="flexcomp_grid_2" type="grid" count="4 4 4" spacing=".2 .2 .2" pos="-2 1 1.5"
                  radius=".005" rgba=".7 0 .7 1" dim="3" mass="5">
            <contact condim="3" solref="0.01 1" solimp=".95 .99 .0001" selfcollide="none"/>
            <edge damping="1"/>
            <elasticity young="5e4" poisson="0"/>
        </flexcomp>

        <!-- flexcomps of type box  -->
        <body name="box_body_1" pos="-1 1 1.5">
            <freejoint/>
            <flexcomp name="flexcomp_box_1" type="box" count="4 4 4" spacing=".2 .2 .2"
                      radius=".005" rgba="1 0 0 1" dim="3" mass="5">
                <contact condim="3" solref="0.01 1" solimp=".95 .99 .0001" selfcollide="none"/>
                <edge damping="1"/>
                <elasticity young="1e4" poisson="0.4"/>
            </flexcomp>
            <inertial pos="0 0 0" mass="1" diaginertia="1e-15 1e-15 1e-15"/>
        </body>

        <body name="box_body_2" pos="0 1 1.5">
            <freejoint/>
            <flexcomp name="flexcomp_box_2" type="box" count="4 4 4" spacing=".2 .2 .2"
                      radius=".005" rgba="0 1 0 1" dim="3" mass="5">
                <contact condim="3" solref="0.01 1" solimp=".95 .99 .0001" selfcollide="none"/>
                <edge damping="1"/>
                <elasticity young="5e4" poisson="0"/>
            </flexcomp>
            <inertial pos="0 0 0" mass="1" diaginertia="1e-15 1e-15 1e-15"/>
        </body>

        <!-- flexcomps of type cylinder -->
        <body name="cylinder_body_1" pos="1 1 1.5">
            <freejoint/>
            <flexcomp name="flexcomp_cylinder_1" type="cylinder" count="4 4 4" spacing=".2 .2 .2"
                      radius=".005" rgba="0 0 1 1" dim="3" mass="5">
                <contact condim="3" solref="0.01 1" solimp=".95 .99 .0001" selfcollide="none"/>
                <edge damping="1"/>
                <elasticity young="1e4" poisson="0.4"/>
            </flexcomp>
            <inertial pos="0 0 0" mass="1" diaginertia="1e-15 1e-15 1e-15"/>
        </body>

        <body name="cylinder_body_2" pos="2 1 1.5">
            <freejoint/>
            <flexcomp name="flexcomp_cylinder_2" type="cylinder" count="4 4 4" spacing=".2 .2 .2"
                      radius=".005" rgba="1 1 0 1" dim="3" mass="5">
                <contact condim="3" solref="0.01 1" solimp=".95 .99 .0001" selfcollide="none"/>
                <edge damping="1"/>
                <elasticity young="5e4" poisson="0"/>
            </flexcomp>
            <inertial pos="0 0 0" mass="1" diaginertia="1e-15 1e-15 1e-15"/>
        </body>

        <!-- flexcomps of type box -->
        <body name="box_body_3" pos="3 1 1.5">
            <freejoint/>
            <flexcomp name="flexcomp_box_3" type="box" count="4 4 4" spacing=".2 .2 .2"
                      radius=".005" rgba="0 1 1 1" dim="3" mass="5">
                <contact condim="3" solref="0.01 1" solimp=".95 .99 .0001" selfcollide="none"/>
                <edge damping="1"/>
                <elasticity young="1e4" poisson="0.4"/>
            </flexcomp>
            <inertial pos="0 0 0" mass="1" diaginertia="1e-15 1e-15 1e-15"/>
        </body>

        <body name="box_body_4" pos="4 1 1.5">
            <freejoint/>
            <flexcomp name="flexcomp_box_4" type="box" count="4 4 4" spacing=".2 .2 .2"
                      radius=".005" rgba="1 0 1 1" dim="3" mass="5">
                <contact condim="3" solref="0.01 1" solimp=".95 .99 .0001" selfcollide="none"/>
                <edge damping="1"/>
                <elasticity young="5e4" poisson="0"/>
            </flexcomp>
            <inertial pos="0 0 0" mass="1" diaginertia="1e-15 1e-15 1e-15"/>
        </body>

    </worldbody>

</mujoco>
